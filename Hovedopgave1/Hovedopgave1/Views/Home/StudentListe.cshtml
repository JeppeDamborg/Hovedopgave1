
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StudentListe</title>
    <link rel="stylesheet" href="~/Content/bootstrap.css" />
    <style>
        .divider{
            width:5px;
            height:auto;
            display:inline-block;
        }

    </style>
</head>
<body>
    <div> 
        <input type="text" placeholder="Søg" />
        <input type="submit" value="Søg" />
    </div>
    <table class="table">
        <tr>
            <th>Navn</th>
            <th>Alder</th>
            <th>Uddannelse</th>
            <th>Semester</th>
            <th>Specialisering</th>
        </tr>

        <tr>
            <td>Tom Hansen</td>
            <td>20</td>
            <td>Tømrer</td>
            <td>4.semester</td>
            <td>Renovering</td>

            <td>@Html.ActionLink("Rediger", "RedigerStudent", "Home", null, new { @class = "btn btn-primary"})</td>
            <td>
                <input type="submit" class="btn btn-primary" value="Slet" />
            </td>
        </tr>

    </table>
</body>
</html>
