
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>VirksomhedListe</title>
    <link rel="stylesheet" href="~/Content/bootstrap.css" />
    <style>
        .divider {
            width: 5px;
            height: auto;
            display: inline-block;
        }
    </style>
</head>
<body>
    <div>
        <input type="text" placeholder="Søg" />
        <input type="submit" value="Søg" />
    </div>
    <table class="table">
        <tr>
            <th>Navn</th>
            <th>KontaktPerson</th>
            <th>Telefon.nr</th>
            <th>E-mail</th>
            <th>Virksomhed Info</th>
            <th>Mulige Opgaver</th>
            <th>Profiler</th>
            <th>OpgaveLøsning</th>
            <th>StyrkePosition</th>            
        </tr>
        <tr>
            <td>Bright Eagle</td>
            <td>John Johnson</td>
            <td>45-xx-xx-xx</td>
            <td>Hello@mail.dk</td>
            <td>El-firma</td>
            <td>Webudvikling</td>
            <td>N/A</td>
            <td>Semester Projekt</td>
            <td>Fødevare</td>

            <td>@Html.ActionLink("Rediger", "RedigerStudent", "Home", null, new { @class = "btn btn-primary" })</td>
            <td><input type="submit" class="btn btn-primary" value="Slet" /></td>

        </tr>



    </table>
</body>
</html>
